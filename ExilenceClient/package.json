{
  "name": "exilence",
  "version": "0.9.20",
  "author": {
    "name": "Viktor Gullmark",
    "email": "viktor.gullmark@hotmail.com"
  },
  "keywords": [
    "angular",
    "angular 6",
    "electron",
    "path of exile",
    "exilence",
    "poe dev",
    "signalr",
    ".net core"
  ],
  "main": "main.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/viktorgullmark/exilence.git"
  },
  "scripts": {
    "postinstall": "node patch.js && npx electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p ng:serve electron:serve",
    "build": "npm run electron:tsc && ng build --base-href .",
    "build:prod": "npm run build -- -c production",
    "build:web": "ng build --prod",
    "ng:serve": "ng serve",
    "electron:tsc": "tsc main.ts",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "release": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac",
    "test": "ng test",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@ngrx/effects": "^6.1.2",
    "@ngrx/entity": "^6.1.2",
    "@ngrx/store": "^6.1.2",
    "@ngrx/store-devtools": "^6.1.2",
    "@sentry/browser": "^4.5.3",
    "@swimlane/ngx-charts": "^8.1.0",
    "d3": "^5.5.0",
    "electron-log": "1.3.0",
    "electron-settings": "^3.2.0",
    "electron-updater": "^4.0.0",
    "electron-window-state": "^5.0.3",
    "export-to-csv": "^0.1.2",
    "lodash": "4.17.11",
    "moment": "^2.22.2",
    "ngx-contextmenu": "^5.1.1",
    "poe-log-monitor": "github:viktorgullmark/poe-log-monitor",
    "read-last-lines": "^1.6.0",
    "rxjs-ratelimiter": "^1.0.1",
    "universal-analytics": "^0.4.20"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.3",
    "@angular/animations": "6.0.3",
    "@angular/cdk": "^6.4.7",
    "@angular/cli": "6.0.3",
    "@angular/common": "6.0.3",
    "@angular/compiler": "6.0.3",
    "@angular/compiler-cli": "6.0.3",
    "@angular/core": "6.0.3",
    "@angular/forms": "6.0.3",
    "@angular/http": "6.0.3",
    "@angular/language-service": "6.0.3",
    "@angular/material": "6.1.0",
    "@angular/platform-browser": "6.0.3",
    "@angular/platform-browser-dynamic": "6.0.3",
    "@angular/router": "6.0.3",
    "@aspnet/signalr": "1.1.0",
    "@ngx-translate/core": "10.0.1",
    "@ngx-translate/http-loader": "3.0.1",
    "@types/d3": "^5.0.0",
    "@types/jasmine": "2.8.7",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.9.4",
    "codelyzer": "4.2.1",
    "core-js": "2.5.6",
    "electron": "4.0.1",
    "electron-builder": "20.36.2",
    "electron-reload": "1.3.0",
    "electron-rebuild": "1.8.2",
    "hammerjs": "^2.0.8",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "jquery": "^3.3.1",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.0",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.1.0",
    "npm-run-all": "4.1.3",
    "npx": "10.2.0",
    "protractor": "5.3.2",
    "rxjs": "6.2.0",
    "rxjs-compat": "6.2.0",
    "signalr": "^2.2.3",
    "ts-node": "6.0.3",
    "tslint": "5.10.0",
    "typescript": "2.7.2",
    "wait-on": "2.1.0",
    "webdriver-manager": "12.0.6",
    "zone.js": "0.8.26"
  }
}
